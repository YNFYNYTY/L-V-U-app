
<h1>HELLO USERS</h1>

<% unless @users.blank? %>
  <ul>
    <% @users.each do |u| %>
    <li>
      <%= u.name %> - <%= u.location_id %> - <%= u.visit_id %> - <%= u.premium %>
       	<strong><%= link_to 'KILL', user_path(u), :method => 'delete', confirm: "Are you sure?" %></strong>
      	<strong><%= link_to 'EDIT', edit_user_path(u) %></strong>
    </li>
    <% end %>
  </ul>
  <% end %>
